<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.eharmony.services.mymatchesservice.store.data">
   <class name="CommunicationDo" table="EHCOMMUNICATION" mutable="false">
      <id name="id" type="long" unsaved-value="0" column="COMMUNICATIONID" access="field"/>
 
      <property name="id" column="COMMUNICATIONID" not-null="true" insert="false" update="false" access="field"/>
	  <property name="candidateChooseMhcs" column="CommCandMHCSTimestamp" type="timestamp"/>
	  <property name="commStartedDate" column="CommStarted" type="timestamp"/>
      <property name="commLastSentByUser" column="COMMLASTSENTBYUSER" type="timestamp" not-null="false"/>
      <property name="commLastSentByCandidate" column="COMMLASTSENTBYCANDIDATE" type="timestamp" not-null="false"/>
      <property name="fastTrackStatus" column="FASTTRACKSTATUS" type="FastTrackStatusEnum" not-null="false"/>
      <property name="fastTrackStage" column="FastTrackStage"/>
	  <property name="fastTrackAvailable" column="FastTrackAvailable" type="FastTrackAvailableEnum" not-null="false" />
      <property name="matchId" column="MATCHID" access="field" />
	  <property name="userChooseMhcs" column="CommUserMHCSTimestamp" type="timestamp"/>
	  
   </class>
</hibernate-mapping>